<script>
  function addToCart(proId){
     $.ajax({
         url:'/addcart/'+proId,
         method:'get',
         success:(response)=>{
             if(response.status){
              swal({  
              title: "Product",  
              text: "Added To Cart!",  
              icon: "success",  
              button: "oh yes!",  
              }); 
           }else{
            swal({  
            title: " Oops!",  
            text: response.response, 
            icon: "error",  
            button: "oh no!",  
            }); 
            console.log(response); 
           }
         }
     })
 }               
</script>
  
